{
    "manifest_version": 3,
    "name": "JSONHelper",
    "description": "Json Helper,a better JSON parsing extension, also provides data extraction based on JsonPath.",
    "version": "0.0.1",
    "action": {
        "default_popup": "popup.html",
        "default_icon": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ],
    "options_page": "options.html"
}